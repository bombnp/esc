{
  "presets": [
    "@babel/typescript",
    // "@babel/env",
    ["@babel/preset-env", { "useBuiltIns": false }]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-transform-runtime"
  ],
  "sourceMaps": true
}
